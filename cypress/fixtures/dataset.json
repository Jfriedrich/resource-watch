      "Maximum observed fire radiative power (FRP)": {
        "dataQuery": "    SELECT frp AS value, acq_date,  ST_AsGeoJSON(the_geom) as geom     FROM VIIRS-Active-Fire-Global-1490086842549  WHERE acq_date >= '{{begin}}' AND acq_date <= '{{end}}'  ORDER BY acq_date DESC  LIMIT 10",
