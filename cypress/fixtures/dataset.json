      },
      "Number of fires with fire radiative power over 200": {
        "dataQuery": "    SELECT frp AS value, acq_date,  ST_AsGeoJSON(the_geom) as geom     FROM VIIRS-Active-Fire-Global-1490086842549  WHERE acq_date >= '{{begin}}' AND acq_date <= '{{end}}'  AND frp >= 200.0 ORDER BY acq_date DESC  LIMIT 10",
        "subscriptionQuery": "    SELECT COUNT(*) AS value  FROM VIIRS-Active-Fire-Global-1490086842549  WHERE acq_date >= '{{begin}}' AND acq_date <= '{{end}}' AND frp >= 200.0 "
